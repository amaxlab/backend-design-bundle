{% block standart_doctype %}<!DOCTYPE html>{% endblock %}
<html>
<head>
    {% block head_start %}{% endblock %}
    {% block standart_charset %}<meta charset="UTF-8" />{% endblock %}
    {% block meta %}<meta http-equiv="X-UA-Compatible" content="IE=edge" />{% endblock %}
    <title>{% block before_title %}{% endblock %}{% block title %}Backend{% endblock %}{% block after_title %}{% endblock %}</title>
    {% block standart_stylesheets %}
        {% stylesheets
            'bundles/backenddesign/css/*'
            filter="cssrewrite"
        %}
        <link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="screen" />
        {% endstylesheets %}
    {% endblock %}
    {% block stylesheets %}{% endblock %}
    {% block standart_jquery %}
        {% javascripts '@BackendDesignBundle/Resources/public/js/jquery/jquery.min.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}
    {% block stnadart_ie9_javascript %}
        <!--[if lt IE 9]>
        {% javascripts '@BackendDesignBundle/Resources/public/js/ie9/*' %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        <![endif]-->
    {% endblock %}
    {% block standart_favicon %}<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />{% endblock %}
    {% block head_end %}{% endblock %}
</head>
<body>
{% block body_start %}{% endblock %}
{% block body %}
    {% block before_nav %}{% endblock %}
    {% block nav %}
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-2 navbar-header navbar-header-brand left-side">
                    {% block navbar_brend_img %}{% endblock%}
                    {% block standart_navbar_brend %}<a href="{{ path('homepage') }}">{% block navbar_brend %}Backend{% endblock %}</a>{% endblock %}
                </div>
                <div class="col-md-10 header-content">
                    <div class="navbar-collapse collapse">
                        {% block main_menu %}
                            {{ mopa_bootstrap_menu('backend_main_menu', {'currentClass': 'active', 'ancestorClass': 'active', 'depth': 1}) }}
                        {% endblock %}
                        {% block page_title_standart %}
                            <h1 class="page-title">{% block page_title %}Backend{% endblock %}</h1>
                        {% endblock %}
                    </div>
                </div>
            </div>
        </div>
    </nav>
    {% endblock %}
    {% block after_nav %}{% endblock %}
    <div class="container-fluid">
        <div class="row">
            {% block standart_affix_menu %}
            <div class="col-md-2 sidebar left-side">
                {% block affix_menu %}
                    {{ mopa_bootstrap_menu('backend_affix_menu', {'currentClass': 'active', 'ancestorClass': 'active', 'depth': 1}) }}
                {% endblock %}
            </div>
            {% endblock %}
            {% block before_content %}{% endblock %}
            {% block standart_content %}
            <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
                {% block content %}{% endblock%}
            </div>
            {% endblock %}
            {% block after_content %}{% endblock %}
        </div>
    </div>
{% endblock %}
{% block standart_javascripts %}
    {% javascripts '@BackendDesignBundle/Resources/public/js/*.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
{% block javascripts %}{% endblock %}
{% block body_end %}{% endblock %}
</body>
</html>